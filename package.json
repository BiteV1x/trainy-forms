{
  "name": "google-forms-monorepo",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev -w server\" \"npm run dev -w client\"",
    "codegen": "npm run generate -w client",
    "codegen:watch": "npm run generate -w client -- --watch"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
